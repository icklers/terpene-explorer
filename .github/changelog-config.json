{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "## 🐛 Bug Fixes",
      "labels": ["fix", "bug"]
    },
    {
      "title": "## 📝 Documentation",
      "labels": ["documentation"]
    },
    {
      "title": "## ⚡ Performance",
      "labels": ["performance"]
    },
    {
      "title": "## 🧪 Tests",
      "labels": ["test"]
    },
    {
      "title": "## 🛠 Dependencies",
      "labels": ["dependencies"]
    }
  ],
  "template": "${{CHANGELOG}}\n\n## 🔗 Links\n- [Full Changelog](${DIFF_URL})\n- [Documentation](https://github.com/${{OWNER}}/${{REPO}}/tree/v${{VERSION}}/docs)",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
  "empty_template": "- No changes",
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365
}
